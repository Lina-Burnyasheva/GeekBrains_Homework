x = num.split(".")
a = int(x[0]) # целая часть
b = int(x[1]) # дробная часть
mult = 1
while (a != 0): # перемножаем числа целой части
    mult = mult * (a % 10)
    a = a // 10
while (b != 0): # перемножаем числа дробной части
    mult = mult * (b % 10)
    b = b // 10
print("Произведение цифр равно:", mult)
